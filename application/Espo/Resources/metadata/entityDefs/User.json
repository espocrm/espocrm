            "foreign": "users",
            "layoutRelationshipsDisabled": true
        },
        "contact": {
            "type": "belongsTo",
            "entity": "Contact",
            "foreign": "portalUser"
        },
        "accounts": {
            "type": "hasMany",
            "entity": "Account",
            "foreign": "portalUsers",
            "relationName": "AccountPortalUser"
        },
        "tasks": {
            "type": "hasMany",
            "entity": "Task",
            "foreign": "assignedUser"
        }
    },
    "collection": {
        "orderBy": "userName",
        "order": "asc",
        "textFilterFields": ["name", "userName"]
    }
}
