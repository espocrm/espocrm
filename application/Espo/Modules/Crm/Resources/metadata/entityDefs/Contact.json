                            "select": ["contactId"],
                            "whereClause": {
                                "deleted": false,
                                "accountId": "{value}"
                            }
                        }
                    }
                },
                "NOT IN": {
                    "whereClause": {
                        "id!=s": {
                            "from": "AccountContact",
                            "select": ["contactId"],
                            "whereClause": {
                                "deleted": false,
                                "accountId": "{value}"
                            }
                        }
                    }
                },
                "IS NULL": {
                    "whereClause": {
                        "accountId": null
                    }
                },
                "IS NOT NULL": {
                    "whereClause": {
